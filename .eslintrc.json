{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"  // Menambahkan Prettier
  ],
  "plugins": ["@typescript-eslint", "prettier"],  // Tambahkan Prettier
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", { "args": "none" }],
    "import/no-unresolved": "error",
    "import/named": "off",
    "no-console": "warn",
    "@typescript-eslint/no-explicit-any": "off",
    "prettier/prettier": "warn"  // Aktifkan Prettier untuk linting
  }
}
